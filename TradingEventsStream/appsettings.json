{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kafka": {
    "BootstrapServers": "localhost:9092",
    "ProducerConfig": {
      "Acks": "All",
      "EnableIdempotence": true,
      "MessageTimeoutMs": 30000,
      "RequestTimeoutMs": 30000,
      "RetryBackoffMs": 1000
    }
  }
}